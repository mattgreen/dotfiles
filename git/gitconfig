[core]
    editor = /usr/bin/vim
    excludesfile = ~/.dotfiles/git/gitignore
[push]
    default = simple
[transfer]
    fsckobjects = true
[fetch]
    fsckobjects = true
[receive]
    fsckObjects = true
[alias]
    cleanup = "!git branch --merged | grep -v master | xargs -n 1 git branch -d"
    undo = reset --soft HEAD~1
    unpushed = log --stat @{u}..HEAD
    branches = branch
    tags = tag
    stashes = stash list
    unstage = reset -q HEAD --
[pull]
    ff = only
